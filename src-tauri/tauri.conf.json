{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "RichChat",
  "version": "0.1.0",
  "identifier": "com.z0gsh1u.richchat",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "RichChat",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "http": {
      "scope": [
        "https://api-one.wallstcn.com/*",
        "https://xueqiu.com/*",
        "https://stock.xueqiu.com/*",
        "https://weibo.com/*"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}